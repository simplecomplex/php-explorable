{
    "name": "simplecomplex/explorable",
    "type": "library",
    "description": "Foreach'ing protected members made simple",
    "keywords": ["utils","PHP"],
    "homepage": "https://github.com/simplecomplex/php-explorable",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Friis Mathiasen",
            "email": "jacob.friis@simplecomplex.net"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "suggest": {
        "simplecomplex/inspect": "Better variable dumps and traces."
    },
    "autoload": {
        "psr-4": {
            "SimpleComplex\\Explorable\\": "src/",
            "SimpleComplex\\Tests\\Explorable\\": "tests/src/"
        }
    }
